<h1><%=gettext "Show User"%></h1>

<ul>

  <li>
    <strong><%=gettext "Username:"%></strong>
    <%= @user.username %>
  </li>

  <li>
    <strong><%=gettext "Hashed password:"%></strong>
    <%= @user.hashed_password %>
  </li>

  <li>
    <strong><%=gettext "Role:"%></strong>
    <%= @user.role %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.user_path(@conn, :edit, @user) %></span>
<span><%= link "Back", to: Routes.user_path(@conn, :index) %></span>
